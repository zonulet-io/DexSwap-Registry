{
  "name": "@dexs/registry",
  "version": "0.1.1",
  "description": "A registry of token lists, curated by the dexswap",
  "main": "truffle-config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Agin-DropDisco/dexswap-registry.git"
  },
  "bugs": {
    "url": "https://github.com/Agin-DropDisco/dexswap-registry/issues"
  },
