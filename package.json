    "dotenv": "^8.2.0",
    "truffle": "^5.1.21",
    "truffle-assertions": "^0.9.2",
    "truffle-plugin-verify": "^0.5.8"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^3.0.0",
    "@openzeppelin/test-helpers": "^0.5.5",
    "chai-bignumber": "^3.0.0",
    "prettier": "^2.0.5",
    "prettier-plugin-solidity": "^1.0.0-alpha.48",
    "solhint": "^2.3.1",
    "solhint-plugin-prettier": "^0.0.4"
